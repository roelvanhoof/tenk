{
  "expo": {
    "name": "TenK",
    "description": "It takes 10k hours to become a master. TenK helps you track your progress to reach your 10k goals!",
    "owner": "Roel van Hoof",
    "slug": "tenk",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tenk",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "RoRe Group",
            "project": "TenK",
            "authToken": "your auth token here"
          }
        }
      ]
    }
  }
}
