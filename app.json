{
  "expo": {
    "name": "TenK",
    "description": "TenK is an expo app for iOS and Android that helps you keep track of the time you spend on reaching your goals.",
    "owner": "Roel van Hoof",
    "slug": "tenk",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tenk",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "rore-group",
            "project": "tenk"
          }
        }
      ]
    }
  }
}